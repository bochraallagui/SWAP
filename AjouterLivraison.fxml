            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="id_livreurField" layoutX="47.0" layoutY="305.0" promptText="id_livreur" />
      <TextField fx:id="date_livraisonField" layoutX="47.0" layoutY="130.0" promptText="date_livraison" />
      <TextField fx:id="prix_livraisonField" layoutX="47.0" layoutY="187.0" promptText="prix_livraison" />
      <Button fx:id="ajouterBtn" layoutX="445.0" layoutY="318.0" mnemonicParsing="false" onAction="#AjouterLivraison" style="-fx-background-color: #252525;" text="Ajouter" textFill="WHITE" />
      <Button fx:id="afficherBtn" layoutX="506.0" layoutY="318.0" mnemonicParsing="false" onAction="#AfficherLivraisons" style="-fx-background-color: #252525;" text="Afficher" textFill="WHITE" />
